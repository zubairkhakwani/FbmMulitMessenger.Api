{
  "manifest_version": 3,
  "name": "Proxy Switcher",
  "version": "1.0",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "webRequest",
    "webRequestAuthProvider",
    "alarms"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
  